{
  "family": "researchnet_task",
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "researchnet_web",
      "image": "researchtriangle/researchnet_web",
      "memory": 128,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "command": [
        "python researchnet/manage.py runserver 0.0.0.0:8000"
      ],

      "links": [
        "postgres:db",
        "mongo:mongo"
      ]
    },
    {
      "name": "postgres",
      "image": "postgres",
      "memory": 128,
      "essential": true
    },
    {
      "name": "mongo",
      "image": "mongo",
      "memory": 128,
      "essential": true
    }
  ]
}