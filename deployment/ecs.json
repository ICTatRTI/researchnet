{
  "family": "researchnet_task",
  "volumes": [
        {
            "name": "pg",
            "host": {
                "sourcePath": "/volumes/data"
            }
        }
  ],
  "containerDefinitions": [
    {
      "name": "researchnet_web",
      "image": "researchtriangle/researchnet_web",
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "links": [
        "postgres:db",
        "mongo:mongo"
      ]
    },
    {
      "name": "postgres",
      "image": "postgres",
      "memory": 256,
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "pg",
          "containerPath": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ]
    },
    {
      "name": "mongo",
      "image": "mongo",
      "memory": 256,
      "essential": true
    }
  ]
}