{
  "family": "researchnet_task",
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "researchnet_web",
      "image": "researchtriangle/researchnet_web",
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "links": [
        "postgres:db"
      ]
    },
    {
      "name": "postgres",
      "image": "postgres",
      "memory": 256,
      "essential": true
    }
  ]
}